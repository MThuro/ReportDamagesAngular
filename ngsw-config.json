{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [{
    "name": "ticket-data",
    "urls": [
      "/ticket-list"
    ],
    "cacheConfig": {
      "strategy": "freshness",
      "maxSize": 100,
      "timeout": "10s"
    }
  }]
}
